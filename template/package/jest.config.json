{
  "setupFilesAfterEnv": [
    "<rootDir>/tests/setup/setup.js"
  ],
  "testMatch": [
    "<rootDir>/tests/**/*.test.js"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "verbose": false,
  "testPathIgnorePatterns": [
    "<rootDir>/tests/setup/",
    "<rootDir>/lib/",
    "<rootDir>/build/"
  ],
  "moduleNameMapper": {
  },
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  }
}